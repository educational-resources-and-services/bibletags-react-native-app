{
  "name": "@bibletags/bibletags-react-native-app",
  "version": "0.2.1",
  "description": "Bible Tags",
  "main": "node_modules/expo/AppEntry.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/educational-resources-and-services/bibletags-react-native-app.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/educational-resources-and-services/bibletags-data/issues"
  },
  "homepage": "https://bibletags.org",
  "author": "Andy Hubert",
  "funding": {
    "type": "individual",
    "url": "https://gofundme.com/f/yarn4-bible-tags"
  },
  "scripts": {
    "dev": "concurrently -k 'npm start' 'cd ../bibletags-data && npm start'",
    "android": "expo start --android",
    "build-android-beta": "npm run sync-versions beta && npm run build-custom-mapping && npm run pre-push-swap && expo build:android --release-channel beta && npm run post-push-restore",
    "build-android-production": "npm run sync-versions production && npm run build-custom-mapping && npm run pre-push-swap && expo build:android --release-channel production && npm run post-push-restore",
    "build-custom-mapping": "node ./src/themes/make-custom-mapping.js",
    "build-ios-beta": "npm run sync-versions beta && npm run build-custom-mapping && npm run pre-push-swap && expo build:ios --release-channel beta && npm run post-push-restore",
    "build-ios-production": "npm run sync-versions production && npm run build-custom-mapping && npm run pre-push-swap && expo build:ios --release-channel production && npm run post-push-restore",
    "change-tenant": "./scripts/changeTenant.sh",
    "confirm": "read -p 'Are you sure? ' -n 1 -r && echo '\n' && [[ $REPLY =~ ^[Yy]$ ]]",
    "ios": "expo start --ios",
    "pre-push-swap": "sed -i .orig \"s/PUSH_DATE_STRING/$(date '+%b %d, %Y at %H:%M')/g\" src/components/major/Drawer.js",
    "post-push-restore": "rm src/components/major/Drawer.js && mv src/components/major/Drawer.js.orig src/components/major/Drawer.js",
    "sync-versions": "node ./scripts/syncVersions.js",
    "push-to-beta": "(npm run confirm -s && npm run sync-versions beta && npm run build-custom-mapping && npm run pre-push-swap && expo publish --release-channel beta && npm run post-push-restore) || true",
    "push-to-production": "(npm run confirm -s && npm run sync-versions production && npm run build-custom-mapping && npm run pre-push-swap && expo publish --release-channel production && npm run post-push-restore) || true",
    "import-bible": "node ./scripts/importUsfmToSqlite.js",
    "setup": "node ./scripts/setup.js",
    "start": "npm run build-custom-mapping && expo start",
    "translate": "node node_modules/inline-i18n/scripts/makeTranslationFiles.js --dir='./transations' --parse-dirs='src node_modules/bibletags-ui-helper/src' && npm run translate-convert-json-to-csv",
    "translate-convert-json-to-csv": "node node_modules/inline-i18n/scripts/convertTranslationJSONtoCSV.js --dir='./transations'",
    "translate-convert-csv-to-json": "node node_modules/inline-i18n/scripts/convertTranslationCSVtoJSON.js --dir='./transations'",
    "update-patch": "npm run go-update-patch -s",
    "update-major": "npm run go-update-major -s",
    "update-minor": "npm run go-update-minor -s",
    "go-update-patch": "echo '-------------------------------------------\nUpdate version (PATCH) and deploy to npm...\n-------------------------------------------\n' && npm run confirm && npm i && npm version patch && npm run publish-to-npm",
    "go-update-minor": "echo '-------------------------------------------\nUpdate version (MINOR) and deploy to npm...\n-------------------------------------------\n' && npm run confirm && npm i && npm version minor && npm run publish-to-npm",
    "go-update-major": "echo '-------------------------------------------\nUpdate version (MAJOR) and deploy to npm...\n-------------------------------------------\n' && npm run confirm && npm i && npm version major && npm run publish-to-npm",
    "publish-to-npm": "npm publish --access public && echo '\nSUCCESS!\n'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.80.0",
    "@bibletags/bibletags-ui-helper": "^0.10.3",
    "@bibletags/bibletags-versification": "^0.3.4",
    "@eva-design/eva": "^2.1.1",
    "@expo/vector-icons": "^10.2.1",
    "@expo/webpack-config": "~0.16.2",
    "@google-cloud/translate": "^7.0.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/hooks": "^2.8.1",
    "@react-native-community/netinfo": "7.1.3",
    "@react-native-community/slider": "4.1.12",
    "@redux-devtools/core": "3.11.0",
    "@sentry/react-native": "^3.4.0",
    "@ui-kitten/components": "^5.1.2",
    "@ui-kitten/metro-config": "^5.1.2",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-expo": "9.0.2",
    "better-sqlite3": "7.5.0",
    "colors": "^1.4.0",
    "concurrently": "^7.1.0",
    "deepmerge": "4.2.2",
    "dotenv": "^16.0.0",
    "expo": "^44.0.0",
    "expo-analytics-amplitude": "~11.1.0",
    "expo-app-loading": "~1.3.0",
    "expo-application": "~4.0.1",
    "expo-asset": "~8.4.6",
    "expo-clipboard": "~2.1.0",
    "expo-constants": "~13.0.1",
    "expo-device": "~4.1.0",
    "expo-file-system": "~13.1.4",
    "expo-keep-awake": "~10.0.1",
    "expo-linking": "~3.0.0",
    "expo-localization": "~12.0.0",
    "expo-navigation-bar": "~1.1.1",
    "expo-splash-screen": "~0.14.1",
    "expo-sqlite": "~10.1.0",
    "expo-store-review": "~5.1.0",
    "expo-updates": "~0.11.6",
    "find-in-files": "0.5.0",
    "fs-extra": "^10.0.1",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "inline-i18n": "^0.1.5",
    "inquirer": "^8.2.4",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "inquirer-file-tree-selection-prompt": "^1.0.19",
    "inquirer-search-checkbox": "^1.0.0",
    "inquirer-search-list": "^1.2.6",
    "moment": "2.29.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-circular-progress": "^1.3.7",
    "react-native-crypto-js": "^1.0.0",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-get-random-values": "~1.7.0",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-shadow": "^1.2.2",
    "react-native-simple-side-menu": "github:educational-resources-and-services/react-native-simple-side-menu#22d795ae9000f829365b347c1d8aa82b48c61a30",
    "react-native-sortable-list": "github:educational-resources-and-services/react-native-sortable-list#9f59e640887e250c0a14db3c9e1b621d984e6c53",
    "react-native-svg": "12.1.1",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.2",
    "react-router-native": "6.2.2",
    "react-use": "17.3.2",
    "reduce-reducers": "1.0.4",
    "redux": "4.1.2",
    "redux-persist": "6.0.0",
    "s3-sync-client": "^3.0.0",
    "sentry-expo": "^4.0.0",
    "spinnies": "^0.5.1",
    "uuid": "^8.3.2"
  }
}
